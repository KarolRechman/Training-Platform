                            <label class="control-label m-auto">Campaigns: </label>
                            <select id="Camps" class="selectpicker dropup" data-live-search="true" data-size="5" title="Choose one" data-style="btn-link" data-width="99%" required data-dropup-auto="false" data-container="body">

                                @foreach (var camp in Model)
                                {
                                    <option value="@camp.Id">
                                        @camp.Name
                                    </option>
                                }

                            </select>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-group m-auto" id="Trainings_Select">
